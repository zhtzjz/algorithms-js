{
  "name": "algorithms-js",
  "version": "1.0.0",
  "description": "algorithms-js",
  "main": "index.js",
  "scripts": {
    "precommit": "pretty-quick --staged --pattern src/** && lint-staged",
    "lint": "eslint ./src --ext .js,.vue",
    "format": "pretty-quick --pattern \"**/*.*(js|vue)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhtzjz/algorithms-js.git"
  },
  "keywords": [
    "eslint",
    "prettire"
  ],
  "author": "zhaohuan <zhaohuan1@360.cn>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhtzjz/algorithms-js/issues"
  },
  "homepage": "https://github.com/zhtzjz/algorithms-js#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husk": "^0.5.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
